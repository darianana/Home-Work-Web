@{
    ViewData["Title"] = "Theater";
}

<h2>Explicit loading</h2>

<table width="100%" class="table table-striped table-bordered table-hover">
    <tr>
        <th>Name</th>
        <th>Type</th>
        <th>Tickets</th>
        <th>Price</th>
    </tr>

    @foreach (var u in Model)
    {
        <tr>
            <td>@u.Name</td>
            <td>@u.St_type.Type</td>
            <td>@u.Tickets</td>
            <td>@u.Price</td>
        </tr>
    }
</table>
